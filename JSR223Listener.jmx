            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.jmeter.threads.JMeterVariables
import org.jsoup.Jsoup
import org.jsoup.nodes.Document
import org.jsoup.nodes.Element
import org.jsoup.select.Elements

import org.apache.jmeter.services.FileServer
import org.apache.commons.io.FileUtils


// Get the HTML response from the previous sampler
String htmlResponse = prev.getResponseDataAsString()

// Parse the HTML using JSoup
Document doc = Jsoup.parse(htmlResponse)

// Extract product information
Elements productElements = doc.select(&quot;tr&quot;) // Select all table rows

// Initialize variables to store extracted data
def productIds = []
def productNames = []

// Iterate through the product elements
for (Element productElement : productElements) {
    Elements tdElements = productElement.select(&quot;td&quot;) // Select all table cells in each row
    if (tdElements.size() == 2) { // Check if there are exactly two cells
        String productId = tdElements.get(0).text() // Get the product ID from the first cell
        String productName = tdElements.get(1).text() // Get the product name from the second cell
        
        // Store the extracted data in variables
        productIds.add(productId)
        productNames.add(productName)
        
        // Perform assertions based on your criteria
        if (productName.contains(&quot;Labrador&quot;)) {
            // Add a Response Assertion for Labrador products
            prev.setSuccessful(true)
            prev.setResponseMessage(&quot;Labrador product found.&quot;)
        }
        
        // You can add more assertions or actions here based on your requirements
    }
}

// Set variables to JMeter variables for later use in your test plan
vars.put(&quot;productIds&quot;, productIds.join(&quot;,&quot;))
vars.put(&quot;productNames&quot;, productNames.join(&quot;,&quot;))

// Print extracted data for debugging purposes
log.info(&quot;Extracted Product IDs: &quot; + vars.get(&quot;productIds&quot;))
log.info(&quot;Extracted Product Names: &quot; + vars.get(&quot;productNames&quot;))


// Get the extracted data from JMeter variables
String productIdsFromVars = vars.get(&quot;productIds&quot;)
String productNamesFromVars = vars.get(&quot;productNames&quot;)

// Define the path and filename for the CSV file
String csvFilePath = &quot;C:\\Vasanth\\apache-jmeter-5.6\\apache-jmeter-5.6\\bin\\jsr223 listener\\output.csv&quot;


// Create a CSV content string
String csvContent = &quot;Product ID,Product Name\n&quot; + productIdsFromVars + &quot;,&quot; + productNamesFromVars + &quot;\n&quot;

try {
    // Write the CSV content to the specified file
    FileUtils.writeStringToFile(new File(csvFilePath), csvContent, &quot;UTF-8&quot;, false)


} catch (Exception ex) {
    log.error(&quot;Error exporting data to CSV file: &quot; + ex.getMessage())
}



</stringProp>
